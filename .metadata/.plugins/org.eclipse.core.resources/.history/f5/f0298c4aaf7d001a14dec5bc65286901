import com.capgemini.librarymanagement.dao.UsersDAOImp;
import com.capgemini.librarymanagement.service.AdminService;
import com.capgemini.librarymanagement.service.AdminServiceImp;
import com.capgemini.librarymanagement.service.UsersService;
import com.capgemini.librarymanagement.service.UsersServiceImp;

public class UserFactory {
	public static UsersDAO getUsersDAO() {
		return new UsersDAOImp();
	}
	public static UsersService  getUsersService() {
		return new UsersServiceImp();
	}
}
